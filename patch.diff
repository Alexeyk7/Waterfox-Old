-            if (U_FAILURE(status)) {
-                break;
-            }
-
-            const UChar *start = tzids;
-            UBool hasNext = TRUE;
-            while (hasNext) {
-                const UChar *end = u_strchr(start, (UChar)0x20);
-                if (end == NULL) {
-                    end = tzids + len;
-                    hasNext = FALSE;
-                }
-                if (canonicalID.compare(start, end - start) == 0) {
